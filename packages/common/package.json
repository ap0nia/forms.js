  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/forms.js",
    "directory": "packages/common"
  },
  "bugs": {
    "url": "https://github.com/ap0nia/forms.js/issues"
  },
  "keywords": [],
  "type": "module",
  "exports": {
    "./store": {
      "types": "./src/store/index.ts",
      "import": "./src/store/index.ts",
      "require": "./src/store/index.ts",
      "default": "./src/store/index.ts"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "import": "./src/utils/*.ts",
      "require": "./src/utils/*.ts",
      "default": "./src/utils/*.ts"
    }
  },
  "typesVersions": {
    "*": {
      "store": [
        "./src/index/store.ts"
      ],
      "utils/*": [
        "./src/utils/*.ts"
      ]
    }
  },
  "scripts": {
    "test": "vitest run"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.5.2",
    "typescript": "^5.1.6"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "pnpm": "^8.0.0",
    "node": ">=18"
  }
}
